{
  "redirects": [
    {
      "source": "/api/:path*",
      "destination": "https://cdn-dev.preoday.com/:path*"
    }
  ],
  "headers": [
    {
      "source": "/api/:path*",
      "headers": [
        { "key": "Access-Control-Allow-Credentials", "value": "true" },
        { "key": "Access-Control-Allow-Origin", "value": "*" }
      ]
    }
  ]
}
